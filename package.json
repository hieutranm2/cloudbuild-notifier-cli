    "@google-cloud/secret-manager": "4.2.2",
    "@google-cloud/service-usage": "2.2.2",
    "@google-cloud/storage": "6.12.0",
    "@inquirer/prompts": "3.0.2",
    "@types/node": "20.4.2",
    "colorette": "2.0.20",
    "commander": "11.0.0",
    "figlet": "1.6.0",
    "googleapis": "123.0.0",
    "nunjucks": "3.2.4",
    "semver": "7.5.4",
    "typescript": "5.1.6",
    "update-notifier-cjs": "5.1.6",
    "yaml": "2.3.1"
